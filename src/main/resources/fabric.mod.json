{
  "schemaVersion": 1,
  "id": "our_villager_discounts",
  "version": "${version}",
  "name": "Our Villager Discounts",
  "description": "Shares positive gossip data between players. This could not be done as a datapack due to a bug concerning how gossip NBT data works.",
  "authors": [
    "Gremlite"
  ],
  "contact": {
    "homepage": "https://github.com/Gremlite/MinecraftMod-OurVillagerDiscounts",
    "issues": "https://github.com/Gremlite/MinecraftMod-OurVillagerDiscounts/issues",
    "sources": "https://github.com/Gremlite/MinecraftMod-OurVillagerDiscounts"
  },
  "license": "The Unlicense",
  "icon": "assets/our_villager_discounts/icon.png",
  "environment": "*",
  "accessWidener": "ourvillagerdiscounts.accesswidener",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "ourvillagerdiscounts.ourvillagerdiscounts.OurVillagerDiscounts"
      }
    ]
  },
  "mixins": [
    "our_villager_discounts.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.13.1+kotlin.2.1.10",
    "minecraft": ">=1.21.4"
  }
}
